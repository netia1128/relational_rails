<div style="text-align:right"><%= link_to "Franchisors Index", "/franchisors"%></div>

<h1>Franchisees</h1>

<th>  Name                      City                      State                       Independent                     Annual Sales                      Initial Fee                     Annual Percent Fee to HQ    Date Created:                                       Date Updated: </th>
<br>
<% @franchisees.each do |franchisee| %>
  <table><%= franchisee.name %>    <%= franchisee.city %>    <%= franchisee.state %>     <%= franchisee.independent %>   <%= franchisee.annual_sales %>    <%= franchisee.initial_fee %>   <%= franchisee.pct_fee %>   <%= franchisee.created_at.strftime('%Y-%m-%d') %>   <%= franchisee.updated_at.strftime('%Y-%m-%d') %></table>
     <div><%= button_to "Update #{franchisee.name}", "/franchisees/#{franchisee.id}/edit", method: :get %>
     <%= button_to "Delete #{franchisee.name}", "/franchisees/#{franchisee.id}", method: :delete %></div>
<% end %>
